{
  "name": "lib-pcg-algots",
  "version": "0.2.4",
  "description": "PCG library implemented using Algorand TypeScript",
  "homepage": "https://github.com/CiottiGiorgio/lib-pcg-avm",
  "license": "MIT",
  "author": "Giorgio Ciotti <gciotti.dev@gmail.com>",
  "files": [
    "lib_pcg"
  ],
  "scripts": {
    "compile-contracts": "algokit compile ts smart_contracts --no-output-arc32 --no-output-source-map --debug-level=0 --out-dir artifacts",
    "lint": "eslint lib_pcg smart_contracts",
    "lint:fix": "eslint lib_pcg smart_contracts --fix",
    "audit": "better-npm-audit audit",
    "format": "prettier --write .",
    "check-types": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22.0",
    "npm": ">=9.0"
  },
  "dependencies": {
    "@algorandfoundation/algorand-typescript": "^1.0.1"
  },
  "devDependencies": {
    "@algorandfoundation/algokit-client-generator": "^6.0.0",
    "@algorandfoundation/algokit-utils": "^9.0.0",
    "@algorandfoundation/algokit-utils-debug": "^1.0.4",
    "@algorandfoundation/puya-ts": "^1.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@tsconfig/node22": "^22.0.0",
    "algosdk": "^3.0.0",
    "better-npm-audit": "^3.11.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.18.0",
    "@eslint/js": "^9.18.0",
    "typescript-eslint": "^8.46.0",
    "prettier": "^3.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "esbuild": "0.25.0"
  }
}
